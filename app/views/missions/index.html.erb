<p id="notice"><%= notice %></p>

<div class="container">
<h1>Missions</h1>
</div>

<div class="container">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th>Weight</th>
      <th>Drone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @missions.each do |mission| %>
      <tr>
        <td><%= mission.name %></td>
        <td><%= mission.location.name %></td>
        <td><%= mission.weight %></td>
        <td><%= mission.drone.id %></td>
<!--        <td><%#= link_to 'Show', mission %></td>-->
<!--        <td><%#= link_to 'Edit', edit_mission_path(mission) %></td>-->
<!--        <td><%#= link_to 'Destroy', mission, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
  </tbody>
</table>

  <%= link_to 'Drone List', drone_drone_list_path %>
</div>
<br>

